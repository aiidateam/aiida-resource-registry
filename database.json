                        "template_variables": {
                            "code_binary_name": {
                                "key_display": "Code name",
                                "type": "list",
                                "options": [
                                    "pw",
                                    "pp",
                                    "ph",
                                    "dos",
                                    "projwfc"
                                ]
                            },
                            "version": {
                                "type": "list",
                                "key_display": "QE version",
                                "options": [
                                    "7.0",
                                    "7.1"
                                ],
                                "default": "7.1"
                            }
                        }
                    }
                }
            }
        },
        "gpu": {
